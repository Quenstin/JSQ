<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mod_root"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginRight="1.0dip">

    <TextView
        android:id="@+id/mod_left_bracket0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="0.25dip"
        android:text="("
        android:textColor="@color/input_layer_1_color"
        android:textSize="25sp" />

    <LinearLayout
        android:id="@+id/mod_horizontal_root0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginLeft="1.0dip"
        android:layout_marginRight="1.0dip"
        android:layout_marginBottom="2.0dip"
        android:gravity="bottom">

        <TextView
            android:layout_width="18.899994dip"
            android:layout_height="30.0dip"
            android:background="@drawable/shape_dotted_stroke" />
    </LinearLayout>

    <TextView
        android:id="@+id/mod_right_bracket0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="0.25dip"
        android:text=")"
        android:textColor="@color/input_layer_1_color"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/mod_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:text="mod"
        android:textColor="@color/input_layer_1_color"
        android:textSize="28.0dip" />

    <TextView
        android:id="@+id/mod_left_bracket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="0.25dip"
        android:text="("
        android:textColor="@color/input_layer_1_color"
        android:textSize="25sp" />

    <LinearLayout
        android:id="@+id/mod_horizontal_root"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginLeft="1.0dip"
        android:layout_marginRight="1.0dip"
        android:layout_marginBottom="2.0dip"
        android:gravity="bottom">

        <TextView
            android:layout_width="18.899994dip"
            android:layout_height="30.0dip"
            android:background="@drawable/shape_dotted_stroke" />
    </LinearLayout>

    <TextView
        android:id="@+id/mod_right_bracket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="0.25dip"
        android:text=")"
        android:textColor="@color/input_layer_1_color"
        android:textSize="25sp" />
</LinearLayout>